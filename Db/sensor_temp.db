record(ai, "$(dev):$(code)$(inst)_TEMP$(sensInst)") {
  field(DESC, "$(desc)")
  field(SCAN, "10 second")
  field(DTYP, "MCHsensor")
  field(INP,  "#C0 S$(sens) @$(link)+TEMP")
  field(PREC, "1")
  field(EGU,  "DegF")
}

record(bi, "$(dev):$(code)$(inst)_TEMP$(sensInst)_P") {
  field(DESC, "Sensor presence")
  field(ZNAM, "Not present")
  field(ONAM, "Present")
  field(VAL,  "1")
  field(PINI, "YES")
}
